{
  "manifest_version": 3,
  "name": "Get XPath",
  "version": "1.0",
  "description": "A brief description of the extension.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}
